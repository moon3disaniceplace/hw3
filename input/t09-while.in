// Testing the break / continue

// All of these are allowed
void main(){
    while (1 < 2) {
        int x = 12;
        if (x > 12) break;
        int y = 7;
        if (y > 7) continue;
        while (y < 7) {
            break;
        }
        break;
    }
}

// Last one is not allowed
void printOK(){
    while (1 < 2) {
        int x = 12;
        if (x > 12) break;
        int y = 7;
        if (y > 7) continue;
        while (y < 7) {
            break;
        }
    }
    break;
}